{
    "id": "antigravity-auth",
    "name": "Antigravity Auth",
    "version": "1.13.1",
    "description": "Use Google's Antigravity IDE subscription with Alma via OAuth. Access Claude and Gemini models directly from your Antigravity subscription.",
    "author": {
        "name": "Alma Community"
    },
    "main": "main.js",
    "engines": {
        "alma": "^0.1.0"
    },
    "type": "provider",
    "permissions": [
        "network:fetch",
        "network:localhost",
        "network:domain:accounts.google.com",
        "network:domain:oauth2.googleapis.com",
        "network:domain:googleapis.com",
        "network:domain:daily-cloudcode-pa.sandbox.googleapis.com",
        "network:domain:autopush-cloudcode-pa.sandbox.googleapis.com",
        "network:domain:cloudcode-pa.googleapis.com",
        "providers:manage",
        "notifications"
    ],
    "activationEvents": [
        "onStartup"
    ],
    "contributes": {
        "providers": [
            {
                "id": "antigravity",
                "name": "Antigravity (Google)",
                "authType": "oauth"
            }
        ],
        "commands": [
            {
                "id": "antigravity-auth.login",
                "title": "Login to Antigravity (Google)"
            },
            {
                "id": "antigravity-auth.logout",
                "title": "Logout from Antigravity"
            }
        ]
    },
    "keywords": [
        "antigravity",
        "google",
        "claude",
        "gemini",
        "oauth"
    ]
}
